            <a href="<%=rootPath%>/video/<%=data[i].url%>">
                <div class="fl">
                    <img src="<%=data[i].cover%>">
                </div>
                <div class="subline-width fr">
                    <span class="subline-title"><i class="i-playTime"></i><%-data[i].title%></span>
                    <span class="subline-date"><span class="news-playTime"><%=data[i].updateTime%></span></span>
                </div>
            </a>
        </div>
        <%}%>
    </div>

    <div class="padded-container">
        <a href="javascript:;" class="show-more-button videoTime-more-button">
